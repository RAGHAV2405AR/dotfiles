{
    "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
    "logo": {
        "type": "small"
    },
    "display": {
        "key": {
            "width": 11
        },
        "bar": {
            "charElapsed": "=",
            "charTotal": "-",
            "width": 13
        },
        "percent": {
            "type": 2
        },
        "separator": "  ",
        "color": {
            "keys": "cyan",
            "title": "blue"
        }
    },
    "modules": [
        "title",
        "separator",
        "uptime",
        "packages",
        "kernel",
        {
            "type": "disk",
            "folders": "/",
            "unit": "gb"
        },
        {
            "type": "memory",
            "unit": "gb"
        },
        {
            "type": "localip",
            "interface": "auto"
        }
//        {
